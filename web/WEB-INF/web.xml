<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addPatient</servlet-name>
        <servlet-class>addPatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addOp</servlet-name>
        <servlet-class>addOp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>medical</servlet-name>
        <servlet-class>medical</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>billan</servlet-name>
        <servlet-class>billan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>validerOp</servlet-name>
        <servlet-class>validerOp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addPatient</servlet-name>
        <url-pattern>/addPatient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addOp</servlet-name>
        <url-pattern>/addOp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>medical</servlet-name>
        <url-pattern>/medical</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>billan</servlet-name>
        <url-pattern>/billan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>validerOp</servlet-name>
        <url-pattern>/validerOp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            120
        </session-timeout>
    </session-config>
</web-app>
